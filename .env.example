APP_NAME="qCT Dashboard"
ENVIRONMENT=dev
DATABASE_URL=postgresql+psycopg2://qct:qct@localhost:5432/qct
AUTH_FAKE_USER=demo_user
AUTH_USERS=demo:demo:Demo Viewer:viewer
AUTH_REALM=qct-dashboard
AUTH_SESSION_SECRET=change-me
AUTH_SESSION_COOKIE=qct_session
AUTH_SESSION_MAX_AGE=28800
DATA_SOURCE=mock
MOCK_DATA=true
ALLOW_PHI=false
ORTHANC_URL=http://localhost:8042
ORTHANC_USERNAME=
ORTHANC_PASSWORD=
LOG_LEVEL=INFO
REQUEST_ID_HEADER=X-Request-ID
CORS_ALLOW_ORIGINS=
CORS_ALLOW_METHODS=GET
CORS_ALLOW_HEADERS=*
CORS_ALLOW_CREDENTIALS=false
READY_CHECK_DB=true
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10
DB_POOL_TIMEOUT=30
DB_POOL_RECYCLE=1800
DB_CONNECT_TIMEOUT=10
DB_STATEMENT_TIMEOUT_MS=0
METRICS_ENABLED=false
METRICS_PATH=/metrics
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=change-me
